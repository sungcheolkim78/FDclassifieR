tic()
rholist <- c(0.1, 0.3, 0.5, 0.7, 0.9)
auclist <- create.auclist(0.6, 0.99, 15)
res <- data.table()
N0 <- 100000
for(r in rholist) {
for (a in auclist) {
y <- create.labels(N=N0, rho=r)
gs <- create.scores.gaussian(y, auc=a)
ds <- build_curve(gs, y)
ds_roc <- roc(y, gs)
ds_ci <- ci(ds_roc)
tmp <- data.table(N=length(y), rho=attr(y,'rho'), auc=a, auc_bac=attr(ds,'auc_bac'),
auprc=attr(ds,'auprc'), th_bac=attr(ds,'th_bac'), rstar=attr(ds,'rstar'),
pxxy=attr(ds,'Pxxy'), pxyy=attr(ds, 'Pxyy'), sig_auc=sqrt(attr(ds,'var_auc')),
sig_auc_delong=(ds_ci[2]-ds_ci[1])/3.92)
res <- rbind(res, tmp)
rm(ds)
rm(ds_roc)
}
}
